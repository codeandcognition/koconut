{
  "name": "koconut",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-flow": "^6.23.0",
    "documentation": "^5.5.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "flow-bin": "^0.49.1",
    "flow-scripts": "^0.3.0",
    "flow-typed": "^2.4.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "jsdoc": "^3.5.5",
    "nodemon": "^1.17.5",
    "react-scripts": "1.0.10"
  },
  "dependencies": {
    "@material-ui/core": "^1.2.2",
    "@material-ui/icons": "^1.1.0",
    "async": "^2.6.1",
    "babel-preset-es2015": "^6.24.1",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.1",
    "brace": "^0.10.0",
    "express": "^4.16.3",
    "firebase": "^5.0.4",
    "highlight.js": "^9.12.0",
    "jest-cli": "^23.2.0",
    "lodash": "^4.17.10",
    "lodash.assign": "^4.2.0",
    "node-sass-chokidar": "0.0.3",
    "npm": "^6.3.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.3.0",
    "react-ace": "^5.10.0",
    "react-dom": "^16.3.0",
    "react-graph-vis": "^1.0.2",
    "react-markdown": "^3.3.4",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-tooltip": "^3.6.1",
    "yarn": "^1.8.0"
  },
  "scripts": {
    "start": "npm-run-all -p start-css start-js",
    "start-all": "npm-run-all -p start-css start-js start-api",
    "build": "yarn run make-flow-nicer && yarn run flow && yarn run build-css && react-scripts build && babel api -d build_node",
    "build-ignore": "yarn run build-css && react-scripts build && babel api -d build_node",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "flow": "flow",
    "make-flow-nicer": "flow-typed install && flow-scripts stub",
    "start-js": "react-scripts start",
    "start-api": "nodemon --exec yarn run babel-node -- ./api/bin/www",
    "start-css": "yarn run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "build-css": "node-sass-chokidar src/ -o src/",
    "start-docs": "documentation serve --watch src/** --config docs/documentation.yml",
    "build-docs": "documentation build src/** -f html -o dev/docs --config docs/documentation.yml",
    "build-docs-md": "documentation build src/** -f md -o docs/documentation.md --config docs/documentation.yml",
    "babel-node": "babel-node --presets=flow"
  },
  "proxy": "http://localhost:3001",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  }
}
