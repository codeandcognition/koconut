{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "target": "codeitz",
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/signin",
        "destination": "/",
        "type": 301
      },
      {
        "source": "/worldview",
        "destination": "/",
        "type": 301
      },
      {
        "source": "/instruction/**",
        "destination": "/",
        "type": 301
      },
      {
        "source": "/practice/**",
        "destination": "/",
        "type": 301
      }
    ]
  }
}
